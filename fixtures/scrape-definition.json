[
    {
      "for": {
        "urls": [
          ".*"
        ]
      },
      "exclude": [
        {
          "type": "CSS",
          "path": ".ios-app-hide"
        }
      ],
      "metadata": {
        "recipeYield": {
          "type": "XPATH",
          "path": "//span[@itemProp='recipeYield']/text()"
        },
        "totalTime": {
          "type": "XPATH",
          "path": "//span[@class='total-time']/text()"
        },
        "calories": {
          "type": "XPATH",
          "path": "//span[@itemProp='calories']/text()"
        },
        "ingredientNames": {
          "type": "XPATH",
          "path": "//li[@itemProp='recipeIngredient']/div/text()"
        },
        "ingredientAmts": {
          "type": "XPATH",
          "path": "//li[@itemProp='recipeIngredient']/div/span/text()"
        }
      }
    }
  ]